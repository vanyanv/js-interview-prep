{
  "name": "js-interview-prep",
  "version": "1.0.0",
  "description": "Comprehensive JavaScript & TypeScript Interview Preparation - 300+ Problems Covering All Areas",
  "main": "test.js",
  "type": "module",
  "scripts": {
    "test": "node test.js",
    "test:watch": "nodemon test.js",
    "test:all": "vitest run",
    "test:problems": "vitest run --project problems",
    "test:realworld": "vitest run --project real-world",
    "test:coverage": "vitest run --coverage",
    "test:vitest": "vitest",
    "generate:tests": "tsx scripts/generate-vitest-tests.ts",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "interview",
    "algorithms",
    "data-structures",
    "leetcode",
    "typescript",
    "react",
    "testing",
    "system-design",
    "performance",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.0",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitest/coverage-v8": "^3.2.0",
    "chalk": "^5.3.0",
    "eslint": "^8.52.0",
    "express": "^4.18.0",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.0"
  }
}
